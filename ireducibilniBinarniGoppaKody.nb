  FormBox[
   FractionBox["1", 
    RowBox[{"g", "(", 
     SubscriptBox["L", "i"], ")"}]], TraditionalForm]]],
 ". \n",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", 
    RowBox[{"(", GridBox[{
       {
        FractionBox["1", 
         RowBox[{"g", "(", 
          SubscriptBox["L", "1"], ")"}]], "0", "\[Ellipsis]", "0"},
       {"0", 
        FractionBox["1", 
         RowBox[{"g", "(", 
          SubscriptBox["L", "2"], ")"}]], "\[Ellipsis]", "0"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]",
         "\[VerticalEllipsis]"},
       {"0", "0", "\[Ellipsis]", 
        FractionBox["1", 
         RowBox[{"g", "(", 
          SubscriptBox["L", "n"], ")"}]]}
      }], ")"}]}], TraditionalForm]]]
}], "Text",
