            RowBox[{"nahodnyExp", "[", "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"lhs", "=", 
            RowBox[{"polMul", "[", 
             RowBox[{
              RowBox[{
              "h", "\[LeftDoubleBracket]", "hi", "\[RightDoubleBracket]"}], 
              ",", 
              RowBox[{"polPow", "[", 
               RowBox[{"gInv", ",", "k"}], "]"}]}], "]"}]}], ";", 
           "\[IndentingNewLine]", "\[IndentingNewLine]", 
           RowBox[{"(*", " ", 
